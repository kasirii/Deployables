        <li>Waist</li>
    </bodyPartGroups>
    <layers>
		<li>Shell</li>
    </layers>
	  <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
  </apparel>
    <thingCategories>
        <li>ApparelUtility</li>
    </thingCategories>
    <tradeTags>
        <li>Clothing</li>
        <li>Armor</li>
    </tradeTags>
</ThingDef>

</Defs>