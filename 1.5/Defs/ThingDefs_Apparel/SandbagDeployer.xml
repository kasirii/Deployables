<?xml version="1.0" encoding="utf-8" ?>
<Defs>	

<ThingDef ParentName="Deployable_Pack" MayRequire="skyarkhangel.HSK">
  <defName>Deployable_SandbagsTowable</defName>
  <label>Sandbags with straps</label>
  <description>Sandbags with straps, that can be used as deployable cover.</description>
	
  <graphicData>
			<texPath>Things/Apparel/BeltWithSandbags/Belt</texPath>
			<graphicClass>Graphic_Single</graphicClass>
  </graphicData>

	<stuffCategories>
		<li>Fabric</li>
	</stuffCategories>

	<statBases>
		<Mass>5</Mass>
		<Bulk>10</Bulk>
		<MarketValue>100</MarketValue>
	</statBases>

	<equippedStatOffsets>
		<MoveSpeed>-0.1</MoveSpeed>
	</equippedStatOffsets>
	
  <comps>
	  <li Class="Deployables.CompProps_CompSpawnCover">
        <coverThingDef>Sandbags</coverThingDef>
      </li>
  </comps>
	
  <apparel>
    <wornGraphicPath>Things/Apparel/BeltWithSandbags/Belt</wornGraphicPath>
  </apparel>
<techLevel>Neolithic</techLevel>
</ThingDef>

</Defs>