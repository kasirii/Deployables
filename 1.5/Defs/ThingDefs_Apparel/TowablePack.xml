<?xml version="1.0" encoding="utf-8" ?>
<Defs>	

<ThingDef ParentName="Deployable_Pack" MayRequire="skyarkhangel.HSK">
  <defName>Deployable_BallistaPack</defName>
  <label>ballista pack</label>
  <description>Foldable medieval ballista that can be deployed and manned in combat.</description>
	<graphicData>
		<texPath>Things/Apparel/OneTimeCoverDevice</texPath>
		<graphicClass>Graphic_Single</graphicClass>
	</graphicData>
	<stuffCategories>
		<li>Woody</li>
	</stuffCategories>
  <comps>
	  <li Class="Deployables.CompProps_CompSpawnCover">
      <coverThingDef>MedievalTimes_Ballista</coverThingDef>
      <spawnOffset>1</spawnOffset>
		<destroyParentOnUse>true</destroyParentOnUse>
		<ownerTag>true</ownerTag>
		<doFlip>false</doFlip>
    </li>
  </comps>
  <apparel>
	  <wornGraphicData>
		  <male>
			  <scale>(1.25,1.25)</scale>
		  </male>
		  <female>
			  <scale>(1.25,1.25)</scale>
		  </female>
		  <thin>
			  <scale>(1.25,1.25)</scale>
		  </thin>
		  <hulk>
			  <scale>(1.25,1.25)</scale>
		  </hulk>
		  <fat>
			  <scale>(1.25,1.25)</scale>
		  </fat>
	  </wornGraphicData>
    <wornGraphicPath>Things/Apparel/Towable/Medieval_Ballista</wornGraphicPath>
  </apparel>
</ThingDef>

