<?xml version="1.0" encoding="utf-8" ?>
<Defs>	

	<ThingDef ParentName="Deployable_Pack" MayRequire="skyarkhangel.HSK">
	  <defName>Deployable_BallistaPack</defName>
	  <label>ballista pack</label>
	  <description>Foldable medieval ballista that can be deployed and manned in combat.</description>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<techLevel>Neolithic</techLevel>
	  <comps>
		  <li Class="Deployables.CompProps_CompSpawnCover">
			  <coverThingList>
				  <li>
					  <thingDef>MedievalTimes_Ballista</thingDef>
				  </li>
			  </coverThingList>
			<spawnOffset>1</spawnOffset>
			<doFlip>false</doFlip>
			<ammoCountRange>
				<min>8</min>
				<max>10</max>
			</ammoCountRange>
			<randomizeAmmo>true</randomizeAmmo>
		  </li>
	  </comps>
	</ThingDef>

	<ThingDef ParentName="Deployable_Pack" MayRequire="skyarkhangel.HSK">
		<defName>Deployable_MachinegunPack</defName>
		<label>Machinegun pack</label>
		<description>Foldable Hi Tech machinegun that can be deployed and manned in combat.</description>
		<stuffCategories>
			<li>StrongMetallic</li>
		</stuffCategories>
		<techLevel>Spacer</techLevel>
		<comps>
			<li Class="Deployables.CompProps_CompSpawnCover">
				<coverThingList>
					<li>
						<thingDef>Turret_M2Browning</thingDef>
					</li>
					<li>
						<thingDef>Turret_Minigun</thingDef>
						<weight>3</weight>
					</li>
				</coverThingList>
				<drawOffset>0.5</drawOffset>
				<spawnOffset>1</spawnOffset>
				<doFlip>false</doFlip>
				<ammoCountRange>
					<min>100</min>
					<max>150</max>
				</ammoCountRange>
				<randomizeAmmo>true</randomizeAmmo>
			</li>
		</comps>
	</ThingDef>

